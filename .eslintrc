---
extends: "airbnb"

parser: "babel-eslint"

plugins:
  - "babel"
  - "flowtype"

env:
  mocha: true

rules:
  flowtype/define-flow-type: 1
  import/no-extraneous-dependencies: 0
  import/prefer-default-export: 0
  no-underscore-dangle: 0
  global-require: 0
  prefer-arrow-callback: 0
  arrow-parens: 0
  no-console: 0
